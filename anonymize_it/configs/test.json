{
  "source": {
    "type": "json_file_reader",
    "params": {
      "filepath": "/opt/anonymize-it/data/*.json"
    }
  },
  "dest":{
    "type": "filesystem",
    "params": {
      "directory": "../output"
    }
  },
  "include": {
    "log.file.path" : "file_path",
    "source.ip": "ipv4",
    "geo": "geo_point",
    "related.ip": "ipv4"
  },
  "exclude": ["user.name"],
  "include_rest": true,
  "anonymizer": "lazy"
}
